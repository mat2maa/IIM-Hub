<tr>
  <td><%= release_counter + 1 %></td>
  <td><%= release.title %></td>
  <td><%= release.type %></td>
  <td><%= release.status %></td>
  <td><%= release.country %></td>
  <td><%= release.date %></td>
  <td><%= release.format %></td>
  <td><%= link_to 'Import',
                  import_release_path(id: release.id,
                                      artist: params[:artist],
                                      artist_id: params[:artist_id],
                                      release_group: params[:release_group]),
                  remote: true,
                  class: 'btn btn-mini',
                  confirm: 'Are you sure?' %>
  </td>
</tr>