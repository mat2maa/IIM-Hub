<table width="300" id="videos" cellspacing="0" cellpadding="0">
	<% Master.find_all_by_video_id(video_id).each do |master| %>
		<tr>
			<td><%= master.episode_title %></td>
			<td width="80">
				<%= link_to "edit", edit_master_path(master), :class => 'edit_master' %> | 
				<%= link_to 'delete', master_path(master), :class => 'delete_master' %>
			</td>
		</tr>
	<% end %>
</table>