<%= render :partial => 'header' %>
<div id="content">
<center>
<h1>New Origin</h1>

<% form_for(@origin) do |f| %>
  <%= error_messages_for(f.object) %>

  <p>
    Origin<br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', origins_path %>

</center>
<br/><br/>
</div>