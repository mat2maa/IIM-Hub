<%= render partial: 'header' %>
<div id="content">
  <center>
    <h1>New airline_rights_country</h1>

    <%= form_for(@airline_rights_country) do |f| %>
      <%= error_messages_for(f.object) %>

      <p>
        <%= f.label :name %><br/>
        <%= f.text_field :name %>
      </p>

      <p>
        <%= f.submit "Create" %>
      </p>
    <% end %>

    <%= link_to 'Back',
                airline_rights_countries_path %>

  </center>
  <br/><br/>
</div>