<%= render :partial => 'header' %>
<div id="content">
<center>
<h1>Edit Video Playlist Type</h1>

<% form_for(@video_playlist_type) do |f| %>
  <%= error_messages_for(f.object) %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Back', video_playlist_types_path %>

</center>
<br/><br/>
</div>