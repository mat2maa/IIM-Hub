<%= render partial: 'header' %>
<div id="content">
  <center>
    <h1>Edit language</h1>

    <%= form_for(@language) do |f| %>
      <%= error_messages_for(f.object) %>

      <p>
        Name<br/>
        <%= f.text_field :name %>
      </p>

      <p>
        <%= f.submit "Update" %>
      </p>
    <% end %>

    <%= link_to 'Back',
                languages_path %>

  </center>
  <br/><br/>
</div>