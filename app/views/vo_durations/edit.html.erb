<%= render partial: 'header' %>
<div id="content">
  <center>
    <h1>Edit vo_duration</h1>

    <%= form_for(@vo_duration) do |f| %>
      <%= error_messages_for(f.object) %>

      <p>
        <%= f.label :name %><br/>
        <%= f.text_field :name %>
      </p>

      <p>
        <%= f.label :duration %><br/>
        <%= f.text_field :duration %>
      </p>

      <p>
        <%= f.submit "Update" %>
      </p>
    <% end %>

    <%= link_to 'Back',
                vo_durations_path %>

  </center>
  <br/><br/>
</div>